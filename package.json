{
  "name": "react-sheet",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run docs:copy-md && vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "docs:db": "node scripts/generateDatabaseFunctionsDocs.js",
    "docs:all": "node scripts/generateAllFunctionsDocs.js",
    "docs:html": "node scripts/generateHtmlDocs.js",
    "docs:generate": "npm run docs:all && npm run docs:html",
    "docs:data": "npx tsx scripts/generateDocsData.ts",
    "docs:build": "npm run docs:data && npm run docs:copy-md && npm run build",
    "docs:copy-md": "node scripts/copyMarkdownFiles.js"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@types/file-saver": "^2.0.7",
    "@types/react-router-dom": "^5.3.3",
    "file-saver": "^2.0.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.0",
    "tailwindcss": "^4.1.11",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0"
  }
}
